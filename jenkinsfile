pipeline {
    agent any
    stages {
        stage('Checkout') {extensions
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/momentomori101/time-tracker.git']]])
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
            // jdfhv
        }
}
